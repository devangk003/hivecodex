# MongoDB connection string
# For local MongoDB: mongodb://localhost:27017/hivecodex
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/hivecodex
$env:MONGO_URI = "mongodb://127.0.0.1:27017/hivecodex"

# JWT secret key for authentication (generate a secure random string)
JWT_SECRET=your-super-secret-jwt-key-here-make-it-long-and-random

# Server port (optional, defaults to 5000)
PORT=5000

# CORS Origins (for production deployment)
# Add your frontend domain here when deploying
CORS_ORIGIN=http://localhost:5173

# Gemini API configuration
# Obtain your key from Google AI Studio
GEMINI_API_KEY=your-gemini-api-key
# Set the model used by the backend AI service
# Examples: gemini-1.5-flash, gemini-1.5-pro
GEMINI_MODEL=gemini-1.5-flash
